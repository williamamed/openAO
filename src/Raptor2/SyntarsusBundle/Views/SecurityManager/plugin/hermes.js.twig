
hermes.on('request-raptor-login',function(data){
    Raptor.markreload=true;
})
Raptor.markreload=false;

if (window.addEventListener) {        
    window.addEventListener("focus", Raptor.onFocusVerify);
} else if (window.attachEvent) {      
    window.attachEvent("onfocus", Raptor.onFocusVerify);
}

hermes.send('request-raptor-login-ok',{login:true})
