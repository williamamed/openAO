getActions:function(){
    var priv=new Array();
    {% for item in privileges %}
        priv.push('{{ item.getName }}');
    {% endfor %}
    {% for item in privilegesOther %}
        priv.push('{{ item.getRoute|replace({'/':'_'}) }}');
    {% endfor %}
    return priv;
},
onFocusVerify:function(){
    if(Raptor.markreload){
     //Raptor.msg.wait('Estamos recargando esta página como mecanismo de protección del módulo de seguridad....')
     window.location.reload()
    }
}
